<div class="wrap">
    <div class="row">
        <div class="col-xs-12">
            <img src="/assets/images/logo.png" alt="Perficient XD"/>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <div class="navbar">
                <div class="navbar-inner">
                    <div class="question">
                        {{#each}}
                        {{#link-to 'question' this}}
                        Question {{id}}
                        {{/link-to}}
                        {{/each}}
                    </div>
                </div>
            </div>
            <div>
                {{outlet}}
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <div class="welcome-text"><h3 style="color:#fff;text-align:center;">Time Remaining</h3></div>
            <div style="text-align:center;"><span id="ms_timer"></span></div>
        </div>
    </div>
</div>


